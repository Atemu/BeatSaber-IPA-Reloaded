{
  "metadata": [{
    "src": [{
      "src": "..",
      "files": [
        "**.csproj"
      ],
      "exclude": [
        "**/IPA.csproj",
        "**/IPA.Tests.csproj",
        "BuildTools/**.csproj",
        "Doorstop/**.csproj"
      ]
    }],
    "dest": "api",
    "disableGitFeatures": false,
    "disableDefaultFilter": false
  }],
  "build": {
    "content": [{
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [{
      "files": [
        "images/**"
      ]
    }],
    "overwrite": [{
      "files": [
        "override/**.md"
      ],
      "exclude": [
        "obj/**",
        "_site/**"
      ]
    }],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default", "templates/darkfx/darkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}